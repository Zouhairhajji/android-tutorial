<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="@dimen/layout_marging"
    android:orientation="horizontal"
    android:gravity="center"
    tools:context=".activities.DashBoardActivity">


    <fragment
        android:id="@+id/list_clients_fragments"
        android:name="esipe.fr.tp1.fragements.ListClientActivity"
        android:layout_width="1dp"
        android:layout_weight="1"
        android:layout_height="match_parent" />


    <fragment
        android:id="@+id/detail_client_fragments"
        android:name="esipe.fr.tp1.fragements.DetailClient"
        android:layout_width="0dp"
        android:layout_weight="2"
        android:layout_height="match_parent"/>


</LinearLayout>
